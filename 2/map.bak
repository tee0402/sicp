#lang sicp

(define (map proc items)
  (if (null? items)
      nil
      (cons (proc (car items))
            (map proc (cdr items)))))

(define (square x) (* x x))
(define (square-list items)
  (if (null? items)
      nil
      (cons ))
(define (square-list items)
  (map (lambda (x) (square x)) items))

(square-list (list 1 2 3 4))